{
  "rules": {
    "notifications": {
      "subscribers": {
        ".read": true,
        ".write": true
      },
      "users": {
        "$uid": {
          ".read": "auth.uid === $uid",
          ".write": "auth.uid === $uid"
        }
      }
    },
    "ratings": {
      "$uid": {
        ".read": "auth.uid === $uid",
        ".write": "auth.uid === $uid"
      }
    }
  }
}
