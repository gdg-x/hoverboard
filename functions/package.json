{
  "name": "hoverboard-functions",
  "description": "Hoverboard serverless functions",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build:index": "cpx ../dist/index.html dist/",
    "build:src": "babel src --out-dir dist --source-maps",
    "build": "npm run clean && concurrently npm:build:*",
    "clean": "rimraf dist",
    "start:index": "npm run build:index -- --watch",
    "start:src": "npm run build:src -- --watch",
    "start": "npm run clean && concurrently --kill-others npm:start:*"
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "@google-cloud/storage": "^1.7.0",
    "child-process-promise": "^2.2.1",
    "cpx": "^1.5.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.0.0",
    "firebase-functions": "^3.8.0",
    "md5": "^2.2.1",
    "mkdirp-promise": "^5.0.1",
    "moment": "^2.27.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "concurrently": "^5.2.0",
    "rimraf": "^3.0.2"
  }
}
