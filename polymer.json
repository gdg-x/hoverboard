{
  "entrypoint": "index.html",
  "shell": "src/hoverboard-app.html",
  "fragments": [
    "src/pages/blog-list-page.html",
    "src/pages/blog-page.html",
    "src/pages/home-page.html",
    "src/pages/post-page.html",
    "src/pages/schedule-page.html",
    "src/pages/speakers-page.html",
    "src/pages/previous-speakers-page.html",
    "src/pages/team-page.html",
    "src/pages/sponsor-page.html",
    "src/pages/faq-page.html",
    "src/pages/coc-page.html",
    "src/lazy-resources.html"
  ],
  "sources": [
    "data/**/*",
    "images/**/*",
    "scripts/**/*",
    "src/**/*",
    "manifest.json",
    "firebase-messaging-sw.js"
  ],
  "extraDependencies": [
    "bower_components/iron-dropdown/iron-dropdown-scroll-manager.html",
    "bower_components/plastic-image/intersection-observer.js",
    "node_modules/@webcomponents/webcomponentsjs/*.js.map",
    "node_modules/@webcomponents/webcomponentsjs/*.js",
    "node_modules/autotrack/autotrack.js.map",
    "node_modules/autotrack/autotrack.js",
    "node_modules/firebase/*",
    "node_modules/redux/dist/redux.min.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "bundle": true,
    "insertPrefetchLinks": true,
    "addServiceWorker": true,
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  }]
}
