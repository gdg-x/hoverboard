{
  "name": "hoverboard",
  "version": "2.1.0",
  "description": "Conference website template",
  "scripts": {
    "gulp": "gulp",
    "bower": "bower",
    "build": "gulp",
    "build:prod": "cross-env BUILD_ENV=production gulp",
    "predeploy": "cd ./functions && npm i",
    "deploy": "gulp deploy",
    "deploy:prod": "cross-env BUILD_ENV=production gulp deploy",
    "donotusefirestore:init": "firebase firestore:delete -r --all-collections && firebase functions:config:set schedule.enabled=true && firebase deploy --except hosting && babel-node ./internals/import-default-data",
    "firestore:copy": "babel-node ./internals/firestore-copy-data",
    "postinstall": "bower install; sh accessibility.sh",
    "serve": "gulp serve",
    "serve:prod": "cross-env BUILD_ENV=production gulp serve",
    "start": "npm run serve",
    "lint": "gulp lint",
    "pretest": "npm run lint",
    "test": "echo \"No tests\""
  },
  "dependencies": {
    "autotrack": "^2.4.1",
    "axios": "^0.19.0",
    "firebase": "^9.6.1",
    "js-yaml": "^3.13.1",
    "npm-check-updates": "^3.1.10",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "bower": "^1.8.2",
    "browser-sync": "^2.26.7",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.1.3",
    "css-slam": "^2.1.2",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-polymer": "^0.1.0",
    "firebase-admin": "^8.0.0",
    "firebase-tools": "^6.11.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-eslint": "^4.0.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^6.0.0",
    "gulp-nunjucks": "^3.1.1",
    "gulp-replace": "^0.6.1",
    "gulp-run": "^1.7.1",
    "gulp-uglify": "3.0.1",
    "merge-stream": "^1.0.1",
    "polymer-build": "^3.1.4",
    "pre-commit": "^1.2.2",
    "require-uncached": "^1.0.3",
    "sw-precache": "^5.2.0",
    "uglify-es": "^3.3.4"
  },
  "private": true,
  "pre-commit": "lint"
}
