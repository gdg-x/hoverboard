{
  "name": "hoverboard",
  "version": "2.1.0",
  "description": "Conference website template",
  "scripts": {
    "gulp": "gulp",
    "bower": "bower",
    "build": "gulp",
    "build:prod": "cross-env BUILD_ENV=production gulp",
    "deploy": "gulp deploy",
    "deploy:prod": "cross-env BUILD_ENV=production gulp deploy",
    "firestore:init": "firebase firestore:delete -r --all-collections -y && firebase functions:config:set schedule.enabled=true && firebase deploy --except hosting && babel-node ./internals/import-default-data",
    "firestore:copy": "babel-node ./internals/firestore-copy-data",
    "postinstall": "npm run install:bower && npm run install:functions && npm run install:emulator",
    "install:bower": "bower install",
    "install:emulator": "firebase setup:emulators:firestore",
    "install:functions": "npm --prefix ./functions install",
    "start": "gulp serve",
    "start:prod": "cross-env BUILD_ENV=production gulp serve",
    "lint": "gulp lint",
    "pretest": "npm run lint",
    "test": "npm run test:rules",
    "test:rules": "firebase emulators:exec --only firestore 'npx jest __tests__/*.rules.test.js'"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^1.3.3",
    "autotrack": "^2.4.1",
    "firebase": "^7.6.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@firebase/testing": "^0.16.4",
    "bower": "^1.8.8",
    "browser-sync": "^2.26.7",
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^5.2.0",
    "css-slam": "^2.1.2",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.13.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-polymer": "^0.4.0",
    "firebase-admin": "^8.9.0",
    "firebase-tools": "^7.11.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-eslint": "^4.0.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-nunjucks": "^3.1.1",
    "gulp-replace": "^0.6.1",
    "gulp-run": "^1.7.1",
    "gulp-uglify": "3.0.2",
    "jest": "^24.9.0",
    "merge-stream": "^1.0.1",
    "polymer-build": "^2.5.0",
    "require-uncached": "^1.0.3",
    "sw-precache": "^5.2.1",
    "uglify-es": "^3.3.9"
  },
  "engines": {
    "node": "10"
  },
  "private": true
}
