    "stylelint-config-recommended": "^4.0.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "contributors": [
    "Oleh Zasadnyy, GDG Lviv",
